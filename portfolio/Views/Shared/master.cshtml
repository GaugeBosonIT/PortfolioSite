@{
  ViewBag.Title = "master";
}
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>@ViewBag.Title</title>
  <link href="@Url.Content("~/Content/1140.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
  <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/raphael-min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/Vegur.font.js")" type="text/javascript"></script>
  <script type="text/javascript">

      $(document).ready(function () {
          $(".jsnavlink").click(function (event) {
              $(".jsnavlink.selected").removeClass("selected");
              $(event.target).addClass("selected");
              $(".container.minheight").removeClass("minheight");

              $("#" + $(event.target).attr("_target")).addClass("minheight")


              $('html, body').animate({
                  scrollTop: $("#" + $(event.target).attr("_target")).offset().top-40
              }, 1000);
              return false;
          });
      });
 
 
  </script>
</head>
<body>
  <div class="fixtop">
    @*colourband*@
    <div class="twelevecol last colourband">
    </div>
    @*navstrip *@
    <div class="headerstrip">
    <div class="row">
      <div class="twelvecol last navstrip">

        <ul class="navlist">
          <li><a _target="contact" class="jsnavlink">Contact</a></li>
          <li><a _target="projects" class="jsnavlink">Projects</a></li>
          <li><a _target="services" class="jsnavlink">Services</a></li>
          <li><a _target="about" class="jsnavlink">About</a></li>
          <li><a _target="home" class="jsnavlink selected">Home</a></li>
        </ul>
       <div class="logo"><span class="eight">8</span><span class="sprints">sprints</span></div>
      </div>
    </div>
    </div>
  </div>
  @RenderBody()
  @*footer*@
  <div class="twelevecol last border">
  </div>
  <div class="footer row">
    <div class="threecol">
      <ul>
        <span class="fontWeightBold">Our work</span>
        <ul>
          <li><a _target="about" class="jsnavlink">Methodology</a></li>
         <li><a _target="services" class="jsnavlink">Services</a></li>

        </ul>
    </div>
    <div class="threecol">
      <ul>
        <span class="fontWeightBold">Company</span>
        <ul>
          <li><a _target="projects" class="jsnavlink">Clients</a></li>
          <li><a _target="projects" class="jsnavlink">Portfolio</a></li>
        </ul>
    </div>
    <div class="threecol">
      <span class="fontWeightBold">Legal</span>
      <ul>
        <li><a _target="imprint" class="jsnavlink">Imprint</a></li>
        <li><a _target="imprint" class="jsnavlink">Privacy</a></li>
      </ul>
    </div>
    <div class="threecol last">
      <span class="fontWeightBold">Connect</span>
      <ul>
        <li><a target="_blank" href="http://www.facebook.com/pages/8sprints/296854413668895?sk=info" class="linkFb">Join us on Facebook</a></li>
        <li><a target="_blank" href="https://twitter.com/#!/8sprints" class="linkTw">Follow us on Twitter</a></li>
  
      </ul>
    </div>
  </div>
  <div class="row">
    <div class="onecol">
    </div>
    <div class="tencol border-top footnote">
       <a href="http://twitter.com/share" class="twitter-share-button" data-url="http://www.8sprints.com" data-count="horizontal">Tweet</a>
      <script type="text/javascript" src="http://platform.twitter.com/widgets.js"></script>
<iframe src="//www.facebook.com/plugins/likebox.php?href=http%3A%2F%2Fwww.facebook.com%2Fpages%2F8sprints%2F296854413668895&amp;width=292&amp;height=62&amp;colorscheme=light&amp;show_faces=false&amp;border_color&amp;stream=false&amp;header=false&amp;appId=181665655236350" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:292px; height:62px;" allowTransparency="true"></iframe>
     <span class="floatRight" style="margin-top: 3px">2011 8sprints. All Rights Reserved.</span>
    </div>
    <div class="onecol">
    </div>
  </div>
</body>
</html>
