@{
  ViewBag.Title = "master";
}
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>@ViewBag.Title</title>
  <link href="@Url.Content("~/Content/1140.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
  <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/raphael-min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/Vegur.font.js")" type="text/javascript"></script>
  <script type="text/javascript">

    $(document).ready(function () {
      $(".jsnavlink").click(function (event) {
        $(".jsnavlink.selected").removeClass("selected");
        $(event.target).addClass("selected");
        $('html, body').animate({
          scrollTop: $("#" + $(event.target).attr("_target")).offset().top
        }, 1000);
        return false;
      });
    });
 
 
  </script>
</head>
<body>
  <div class="fixtop">
    @*colourband*@
    <div class="twelevecol last colourband">
    </div>
    @*navstrip *@
    <div class="row">
      <div class="twelvecol last navstrip">
        <ul class="navlist">
          <li><a _target="contact" class="jsnavlink">Contact</a></li>
          <li><a _target="projects" class="jsnavlink">Projects</a></li>
          <li><a _target="services" class="jsnavlink">Services</a></li>
          <li><a _target="about" class="jsnavlink">About</a></li>
          <li><a _target="home" class="jsnavlink selected">Home</a></li>
        </ul>
      </div>
    </div>
  </div>
  @RenderBody()
  @*footer*@
  <div class="twelevecol last border">
  </div>
  <div class="footer row">
    <div class="threecol">
      <ul>
        <span class="fontWeightBold">Our work</span>
        <ul>
          <li><a href="/Services">Methodology</a></li>
          <li><a href="/Clients">FAQ</a></li>
          <li><a href="/Portfolio">Testimonials</a></li>
          <li><a href="/Jobs">Jobs</a></li>
        </ul>
    </div>
    <div class="threecol">
      <ul>
        <span class="fontWeightBold">Company</span>
        <ul>
          <li><a href="/Clients">Clients</a></li>
          <li><a href="/Portfolio">Portfolio</a></li>
          <li><a href="/Jobs">Jobs</a></li>
        </ul>
    </div>
    <div class="threecol">
      <span class="fontWeightBold">Legal</span>
      <ul>
        <li><a href="/Services">Imprint</a></li>
        <li><a href="/Clients">Privacy</a></li>
      </ul>
    </div>
    <div class="threecol last">
      <span class="fontWeightBold">Connect</span>
      <ul>
        <li><a target="_blank" href="http://www.facebook.com/friendfund" class="linkFb">Join us on Facebook</a></li>
        <li><a target="_blank" href="http://twitter.com/#!/friendfund" class="linkTw">Follow us on Twitter</a></li>
        <li><a target="_blank" href="http://blog.friendfund.com/?feed=rss2" class="linkRss">Subscribe to our Blog</a></li>
      </ul>
    </div>
  </div>
  <div class="row">
    <div class="twocol">
    </div>
    <div class="eightcol border-top footnote">
      <span>Share Friendfund</span> <a href="http://twitter.com/share" class="twitter-share-button" data-url="http://www.friendfund.com" data-count="horizontal">Tweet</a>
      <script type="text/javascript" src="http://platform.twitter.com/widgets.js"></script>
      <iframe src="https://www.facebook.com/plugins/like.php?href=www.facebook.com%2Ffriendfund&amp;layout=button_count&amp;show_faces=false&amp;width=200&amp;action=like&amp;font=arial&amp;colorscheme=light&amp;height=21"
        scrolling="no" frameborder="0" style="border: none; overflow: hidden; width: 120px; height: 21px;" allowtransparency="true"></iframe>
      <span class="floatLeft" style="margin-top: -13px"><a target="_blank" href="https://trustsealinfo.verisign.com/splash?form_file=fdf/splash.fdf&dn=www.friendfund.com&lang=en">
        <img src="/static/imgs/veri-sign_trans.png" /></a></span> <span class="floatRight" style="margin-top: 3px">2011 Someother GmbH. All Rights Reserved.</span>
    </div>
    <div class="twocol">
    </div>
  </div>
</body>
</html>
